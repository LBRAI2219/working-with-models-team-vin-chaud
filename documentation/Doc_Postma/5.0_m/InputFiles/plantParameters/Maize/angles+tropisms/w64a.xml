<SimulationModelIncludeFile>			
		<SimulaDirective path="hypocotyl">
				<SimulaConstant name="numberOfXylemPoles" type="int">
					 61 
				</SimulaConstant>
				<!-- v1 gives a relative maximum lenth of the soil impedence vector
				     v2 gives a soil impedence vector for which it's length is relative to the growth vector-->
				<SimulaConstant name="soilImpedence" > 0.3</SimulaConstant>
				<SimulaConstant name="gravitropism" > -1. </SimulaConstant>
				<SimulaStochastic name="soilImpedence.v2" unit="cm" type="Coordinate" distribution="uniform"  minimum="-0.3" maximum="0.3"/> 
				<SimulaStochastic name="gravitropism.v2" unit="cm"  type="Coordinate" distribution="uniform"  minimum="0.5" maximum="1"/> 
		</SimulaDirective>
		<SimulaDirective path="primaryRoot">
				<SimulaConstant name="branchingAngle"  unit="degrees">
					 0 <!--value unknown-->
				</SimulaConstant>
				<SimulaConstant name="numberOfXylemPoles" type="int">
					 8 
					 <!-- 6-10 xylem poles in primary-seminal roots and increasing more in crown & brace roots up to 48 
					 monocotolydons roots branch from phloem poles, not xylem
					 Hochholdinger F. 2009. The Maize Root System: Morphology, Anatomy, and Genetics. Handbook of Maize: Its Biology.145-160.
					 -->
				</SimulaConstant>
				<SimulaConstant name="soilImpedence" > 0.05	</SimulaConstant>
				<SimulaConstant name="gravitropism" > 0.01 </SimulaConstant>
				<SimulaStochastic name="soilImpedence.v2" unit="cm" type="Coordinate" distribution="uniform"  minimum="-0.05" maximum="0.05"/> 
				<SimulaStochastic name="gravitropism.v2" unit="cm"  type="Coordinate" distribution="uniform"  minimum="-0.015" maximum="-0.005"/> 
		</SimulaDirective>
