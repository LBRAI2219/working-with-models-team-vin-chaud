<!--SimRoot parameter file-->
<SimulationModelIncludeFile>
	<SimulaDirective path="environment/soil">
		<SimulaBase name="water">
			<SimulaTable
				name_colum2="initialHydraulicHead"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="cm"> 0 -30 -30 0
			</SimulaTable>
			<SimulaTable
				name_colum2="residualWaterContent"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="100%"> 0 0.07 -300 0.07
			</SimulaTable>
			<SimulaTable
				name_colum2="saturatedWaterContent"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="100%"
				TinterpolationMethod="step"> 0 0.30 -100 0.30
			</SimulaTable>
			<SimulaTable
				name_colum2="vanGenuchten:alpha"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="1/cm"> 0 0.03 -300 0.03
			</SimulaTable>
			<SimulaTable
				name_colum2="vanGenuchten:n"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="noUnit"> 0 2. -300 2.
			</SimulaTable>
			<SimulaTable
				name_colum2="saturatedConductivity"
				name_colum1="depth"
				unit_colum1="cm"
				unit_colum2="cm/day"
				TinterpolationMethod="step"> 0 40 -300 40
			</SimulaTable>
			<SimulaConstant
				name="volumetricWaterContentInBarberCushman"
				unit="cm3/cm3">0.102</SimulaConstant>
		</SimulaBase>
	</SimulaDirective>
</SimulationModelIncludeFile>